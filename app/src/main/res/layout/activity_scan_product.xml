<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/tpLayout"
    android:orientation="vertical"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:keepScreenOn="true"
    tools:context=".Functionality.barcodescan.ScanProduct">

    <com.example.zeeshanhussain.qwicklyapppart1.Functionality.ui.camera.CameraSourcePreview
        android:id="@+id/preview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        >
        <com.example.zeeshanhussain.qwicklyapppart1.Functionality.ui.camera.GraphicOverlay
            android:id="@+id/graphicOverlay"
            android:layout_width="match_parent"
            android:layout_height="match_parent"

            />
    </com.example.zeeshanhussain.qwicklyapppart1.Functionality.ui.camera.CameraSourcePreview
        >
</LinearLayout>